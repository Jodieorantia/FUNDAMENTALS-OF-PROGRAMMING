#include <stdio.h>

int main() 
{
    int grade;
    
    printf("Enter your grade (0 - 100): ");
    scanf("%d", &grade);
    
    if  (grade < 0 /grade > 100) {
        printf("Invalid grade\n");
        return 0;
    }

    switch (grade / 10) {
        case 10: 
        case 9:  
            printf("Equivalent Grade: A\n");
            break;
        case 8:  
            printf("Equivalent Grade: B\n");
            break;
        case 7:  
        case 6:  
            printf("Equivalent Grade: C\n");
            break;
        default: 
            printf("Equivalent Grade: Failed\n");
            break;
    }

    return 0;
}